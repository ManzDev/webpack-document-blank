{
  "name": "webpack-document-blank",
  "version": "1.0.0",
  "description": "A prepared package for new HTML/CSS/JS projects and easy coding (with Webpack)",
  "author": "Manz",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js -w",
    "lint:css": "stylelint 'src/**/*.css' || true",
    "lint:js": "eslint 'src/**/*.js' || true",
    "lint:ts": "eslint 'src/**/*.ts' || true",
    "lint": "npm-run-all lint:*",
    "clean:cache": "rm -rf dist build",
    "clean:all": "rm -rf node_modules package-lock.json dist build",
    "build:gh-pages": "rm -rf docs && webpack --config webpack.prod.js && mv build/ docs/",
    "deploy:gh-pages": "git subtree push --prefix docs origin gh-pages"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^2.3.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.0"
  },
  "keywords": [
    "webpack",
    "template",
    "typescript"
  ],
  "license": "ISC"
}
